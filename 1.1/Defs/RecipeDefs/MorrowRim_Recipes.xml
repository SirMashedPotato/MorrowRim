<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Muck to medicine -->
	
	<RecipeDef>
		<defName>MorrowRim_MakeMedicince_FromMuck</defName>
		<label>refine muck into herbal medicine</label>
		<description>Refines 20 buckets of muck into 4 herbal medicine.</description>
		<jobString>Refining herbal medicine from muck.</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>500</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<recipeUsers>
			<li>CraftingSpot</li>
			<li>DrugLab</li>
		</recipeUsers>
		<ingredients>
		  <li>
			<filter>
			  <thingDefs>
				<li>MorrowRim_Muck</li>
			  </thingDefs>
			</filter>
			<count>20</count>
		  </li>
		</ingredients>
		<fixedIngredientFilter>
		  <thingDefs>
			<li>MorrowRim_Muck</li>
		  </thingDefs>
		</fixedIngredientFilter>
		<products>
		  <MedicineHerbal>4</MedicineHerbal>
		</products>
		<researchPrerequisite>MorrowRim_MuckRefining_Wasteful</researchPrerequisite>
		<workSkill>Medicine</workSkill>
	</RecipeDef>
	
