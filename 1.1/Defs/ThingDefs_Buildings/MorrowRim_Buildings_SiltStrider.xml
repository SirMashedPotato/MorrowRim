<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="BuildingBase">
		<defName>MorrowRim_SiltStriderShell_a</defName>
		<label>silt strider shell</label>
		<description>The empty shell of a silt strider. Can be deconstructed for silt strider chitin.</description>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>MorrowRim/Buildings/Dessicated_SiltStrider_a</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(5.8, 3.4)</drawSize>
		</graphicData>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<rotatable>false</rotatable>
		<costList>
			<MorrowRim_SiltStriderChitin>180</MorrowRim_SiltStriderChitin>
		</costList>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<Mass>4</Mass>
			<Flammability>0.1</Flammability>
			<MarketValue>50</MarketValue>
			<WorkToBuild>2000</WorkToBuild>
		</statBases>
		<fillPercent>0.5</fillPercent>
		<destroyable>true</destroyable>
		<useHitPoints>true</useHitPoints>
		<tickerType>Normal</tickerType>
		<selectable>true</selectable>
		<size>(3,2)</size>
		<blockPlants>true</blockPlants>
	</ThingDef>
	
