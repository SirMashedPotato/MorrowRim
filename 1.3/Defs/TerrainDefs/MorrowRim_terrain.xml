<Defs>

	<!-- Ash -->
	
	<TerrainDef ParentName="NaturalTerrainBase">
		<defName>MorrowRim_Ash</defName>
		<label>ash covered soil</label>
		<texturePath>MorrowRim/Terrain/MorrowRim_ash</texturePath>
		<edgeType>FadeRough</edgeType>
		<renderPrecedence>350</renderPrecedence>
		<pathCost>4</pathCost>
		<statBases>
		  <Beauty>-1</Beauty>
		  <Cleanliness>-1</Cleanliness>
		</statBases>
		<scatterType>SoftGray</scatterType>
		<affordances>
		  <li>Light</li>
		  <li>Medium</li>
		  <li>Heavy</li>
		  <li>GrowSoil</li>
		  <li>Diggable</li>
		</affordances>
		<generatedFilth>Filth_Dirt</generatedFilth>
		<fertility>1.3</fertility>
		<takeFootprints>True</takeFootprints>
		<tools>
		  <li>
			<label>ash</label>
			<capacities>
			  <li>KickMaterialInEyes</li>
			</capacities>
			<hediff>DirtInEyes</hediff>
			<cooldownTime>1.5</cooldownTime>
		  </li>
		</tools>
		<tags>
		  <li>Soil</li>
		</tags>
	</TerrainDef>

