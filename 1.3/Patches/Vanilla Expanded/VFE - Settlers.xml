<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Settlers</li>
		</mods> 
				
		<match Class="PatchOperationSequence">
			<operations>
			
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/FactionDef[@Name="SettlerFactionBase"]/pawnGroupMakers/li[kindDef="Trader"]/carriers</xpath>
					<value>
						<MorrowRim_Guar>10</MorrowRim_Guar>
						<MorrowRim_NixOx>5</MorrowRim_NixOx>
					</value>
				</li>

			</operations>
		</match>
	</Operation>
	
</Patch>

