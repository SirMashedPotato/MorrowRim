<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Androids</li>
		</mods> 
				
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="ChjAndroid"]</xpath>
					<value>
						<li Class="MorrowRim.ExtendedRaceProperties">
							<ashResistant>Mechanical</ashResistant>
						</li>
					</value>
				</li>	
				
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/AlienRace.ThingDef_AlienRace[defName="ChjDroid"]</xpath>
					<value>
						<li Class="MorrowRim.ExtendedRaceProperties">
							<ashResistant>Mechanical</ashResistant>
						</li>
					</value>
				</li>	
				
			</operations>
		</match>
	</Operation>
	
</Patch>

