<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alien Vs Predator</li>
		</mods> 
				
		<match Class="PatchOperationSequence">
			<operations>
					
				<!-- give select animals ash-resistant -->
				
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/ThingDef[@Name="AvP_BaseXenomorphPawn" or @Name="AvP_BaseNeomorphPawn"]</xpath>
					<value>
						<li Class="MorrowRim.ExtendedRaceProperties">
							<ashResistant>Resistant</ashResistant>
						</li>
					</value>
				</li>	

			</operations>
		</match>
	</Operation>
	
</Patch>

